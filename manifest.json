{
  "name": "POLO",
  "description": "A crypto wallet for Polkadot based chains and their parachains",
  "version": "1.0.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "./public/index.html",
    "default_title": "POLO"
  },
  "icons": {
    "16": "./public/logo192.png",
    "48": "./public/logo192.png",
    "128": "./public/logo192.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "xord.innovations@gmail.com",
      "strict_min_version": "42.0",
      "strict_max_version": "50.*"
    }
  }
}
